<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressView">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="backgroundDrawable" format="integer" />
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="currentProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="FeedbackView">
        <attr name="pvText" format="string" />
        <attr name="pvTextSize" format="dimension" />
        <attr name="pvTextColor" format="color" />
        <attr name="pvBackgroundDrawable" format="integer" />
        <attr name="pvMinValue" format="integer" />
        <attr name="pvMaxValue" format="integer" />
        <attr name="pvCurrentProgress" format="integer" />
        <attr name="startButtonDrawable" format="integer" />
        <attr name="endButtonDrawable" format="integer" />
        <attr name="endButtonText" format="string" />
        <attr name="endButtonTextSize" format="dimension" />
        <attr name="endButtonTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ContentView">
        <attr name="cvText" format="string" />
        <attr name="cvTextColor" format="color" />
        <attr name="cvTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ActionView">
        <attr name="avWidth" format="dimension" />
        <attr name="avHeight" format="dimension" />
        <attr name="avMarginHorizontal" format="dimension" />
        <attr name="avMarginVertical" format="dimension" />
        <attr name="avTextColor" format="color" />
        <attr name="avTextSize" format="dimension" />
        <attr name="avBackgroundDrawable" format="reference" />
    </declare-styleable>
</resources>