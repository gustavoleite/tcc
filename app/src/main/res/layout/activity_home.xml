<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.HomeActivity">

    <gustavo.guterres.leite.tcc.components.progress.ProgressComponent
        android:id="@+id/home_progress_component"
        android:layout_width="match_parent"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        app:backgroundDrawable="@drawable/load_progress"
        app:currentProgress="60"
        app:textSize="12sp"
        app:textColor="@color/primaryTextColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:text="5/10" />

</androidx.constraintlayout.widget.ConstraintLayout>